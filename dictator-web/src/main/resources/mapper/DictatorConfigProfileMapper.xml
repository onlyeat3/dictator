<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.github.liuyuyu.dictator.server.mapper.DictatorConfigProfileMapper">
    <select id="findByRoleIdList" resultType="java.lang.Long">
        select * from dictator_role_profile
        where role_id in
        <foreach collection="roleIdList" open="(" separator="," close=")" item="roleId">
            #{roleId}
        </foreach>
    </select>
</mapper>